<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:dist="http://schemas.android.com/apk/distribution"
    package="com.example.meubizu">

    <dist:module dist:instant="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".acontecimentoAgendaActivity"
            android:parentActivityName=".agendaActivity"></activity>
        <activity android:name=".rascunhosActivity"
            android:parentActivityName=".mainActivity" />
        <activity android:name=".geografiaActivity"
            android:parentActivityName=".rascunhosActivity"/>
        <activity android:name=".historiaActivity"
            android:parentActivityName=".rascunhosActivity"/>
        <activity android:name=".filosofiaActivity"
            android:parentActivityName=".filosofiaActivity"/>
        <activity android:name=".sociologiaActivity"
            android:parentActivityName=".rascunhosActivity"/>
        <activity android:name=".linguagensActivity"
            android:parentActivityName=".rascunhosActivity" />
        <activity android:name=".matematicaActivity"
            android:parentActivityName=".rascunhosActivity"/>
        <activity android:name=".agendaActivity"
            android:parentActivityName=".mainActivity"/>
        <activity android:name=".mainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".biologiaActivity"
            android:parentActivityName=".rascunhosActivity"/>
        <activity android:name=".espanholActivity"
            android:parentActivityName=".rascunhosActivity"/>
        <activity android:name=".fisicaActivity"
            android:parentActivityName=".rascunhosActivity"/>
        <activity android:name=".formularioActivity"
            android:parentActivityName=".rascunhosActivity"/>
        <activity android:name=".inglesActivity"
            android:parentActivityName=".rascunhosActivity"/>
        <activity android:name=".portuguesActivity"
            android:parentActivityName=".rascunhosActivity"/>
        <activity android:name=".quimicaActivity"
            android:parentActivityName=".rascunhosActivity"/>
        <activity android:name=".direitosAutoraisActivity"
            android:parentActivityName=".mainActivity"/>
        <activity android:name=".cronogramaActivity"
            android:parentActivityName=".mainActivity"/>
    </application>

</manifest>